{
    "version": 2,
    "builds": [
      {
        "src": "server/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/server/static/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  